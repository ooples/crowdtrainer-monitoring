# Monitoring Dashboard Configuration

# API Configuration
MONITORING_API_URL=http://localhost:3000/api
MONITORING_API_KEY=your-api-key-here

# Data Source Configuration
ENABLE_MULTIPLE_SOURCES=true
PRIMARY_DATA_SOURCE=crowdtrainer
SECONDARY_DATA_SOURCE=external

# White Label Configuration
BRAND_NAME=Monitoring Dashboard
BRAND_LOGO_URL=/logo.png
PRIMARY_COLOR=#3b82f6
SECONDARY_COLOR=#06b6d4
ACCENT_COLOR=#8b5cf6

# Authentication
REQUIRE_API_KEY=true
ADMIN_API_KEY=admin-key-here

# Features
ENABLE_EXPORT=true
ENABLE_REALTIME=true
ENABLE_ALERTS=true
ENABLE_OAUTH_MONITORING=true

# Embedding
ALLOWED_EMBED_ORIGINS=*
EMBED_TIMEOUT=30000

# Performance
CACHE_TTL=300
MAX_EVENTS_PER_REQUEST=100