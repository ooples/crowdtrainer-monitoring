{
  "name": "@monitoring-service/debugging",
  "version": "1.0.0",
  "type": "module",
  "description": "Advanced debugging tools for monitoring service with distributed tracing, session replay, and ML-powered error clustering",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "debugging",
    "tracing",
    "session-replay",
    "error-clustering",
    "performance-profiling",
    "log-correlation",
    "opentelemetry"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "@monitoring-service/core": "workspace:*",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/semantic-conventions": "^1.17.0",
    "@opentelemetry/exporter-jaeger": "^1.17.0",
    "@opentelemetry/exporter-zipkin": "^1.17.0",
    "@opentelemetry/instrumentation": "^0.45.1",
    "@opentelemetry/instrumentation-http": "^0.45.1",
    "@opentelemetry/instrumentation-express": "^0.34.1",
    "simple-git": "^3.20.0",
    "ml-kmeans": "^6.0.0",
    "similarity": "^1.2.1",
    "lz-string": "^1.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.3.0",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}