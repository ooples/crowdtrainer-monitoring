{
  "name": "@monitoring-service/sdk-js",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript SDK for monitoring service with auto-capture and manual tracking",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.js",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "monitoring",
    "analytics",
    "performance",
    "error-tracking",
    "web-vitals",
    "observability"
  ],
  "author": "Monitoring Service",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monitoring-service/sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/monitoring-service/sdk-js/issues"
  },
  "homepage": "https://github.com/monitoring-service/sdk-js#readme",
  "sideEffects": false
}
