{
  "name": "react-native-monitoring-example",
  "version": "1.0.0",
  "description": "React Native mobile app integration example for CrowdTrainer monitoring service",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "pods": "cd ios && pod install && cd ..",
    "clean": "react-native clean-project-auto",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ReactNativeMonitoringExample.xcworkspace -scheme ReactNativeMonitoringExample -configuration Release archive"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "react-native-screens": "^3.22.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-vector-icons": "^10.0.0",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "react-native-device-info": "^10.8.0",
    "react-native-network-info": "^5.2.1",
    "react-native-background-job": "^0.2.2",
    "@react-native-community/netinfo": "^9.4.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-performance": "^5.1.0",
    "@crowdtrainer/monitoring-core": "^1.0.0",
    "@crowdtrainer/monitoring-react-native": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "keywords": [
    "monitoring",
    "react-native",
    "mobile",
    "crowdtrainer",
    "analytics",
    "crash-reporting",
    "performance"
  ],
  "author": "CrowdTrainer Team",
  "license": "MIT",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "jest": {
    "preset": "react-native"
  }
}