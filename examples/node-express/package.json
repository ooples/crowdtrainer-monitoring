{
  "name": "node-express-monitoring-example",
  "version": "1.0.0",
  "description": "Node.js Express backend integration example for CrowdTrainer monitoring service",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js",
    "format": "prettier --write src/**/*.js",
    "docker:build": "docker build -t node-express-monitoring .",
    "docker:run": "docker run -p 3000:3000 node-express-monitoring"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.7.1",
    "express-validator": "^7.0.1",
    "winston": "^3.10.0",
    "axios": "^1.4.0",
    "node-cron": "^3.0.2",
    "sqlite3": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.1",
    "dotenv": "^16.3.1",
    "@crowdtrainer/monitoring-core": "^1.0.0",
    "@crowdtrainer/monitoring-express": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "monitoring",
    "nodejs",
    "express",
    "backend",
    "crowdtrainer",
    "api",
    "analytics"
  ],
  "author": "CrowdTrainer Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}